  <h1>All Beers</h1>
<div class="informations-container">
  {{#each beers}}
    <div class="beer-information">
      <img class="beer-information__image" src="{{this.image_url}}" alt="">
      <h1 class="beer-information_name">{{this.name}}</h1>
      <h2 class="beer-information__tagline">{{this.tagline}}</h2>
      <p class="beer-information__description">{{this.description}}</p>
    </div>
  {{/each}}
</div>